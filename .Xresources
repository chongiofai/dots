! URxvt
URxvt.perl-ext-common: default,clipboard
URxvt.scrollBar: false
URxvt.keysym.Control-Up:         font-size:increase
URxvt.keysym.Control-Down:       font-size:decrease
URxvt.keysym.Control-Shift-Up:   font-size:incglobal
URxvt.keysym.Control-Shift-Down: font-size:decglobal
URxvt.keysym.Control-equal:      font-size:reset
URxvt.keysym.Control-slash:      font-size:show
URxvt.keysym.Shift-Control-V :   eval:paste_clipboard
URxvt.keysym.Shift-Control-C:    eval:selection_to_clipboard
URxvt.keysym.Control-Meta-c:     builtin-string:
URxvt.keysym.Control-Meta-v:     builtin-string:
