*.colorBD:     #ffffff

! xft
*.font:      xft:JetBrains Mono:style=Regular:size=12, xft:WenQuanYi Micro Hei Mono, xft:Symbola
*.boldFont:  xft:JetBrains Mono:style=Bold:size=12, xft:WenQuanYi Micro Hei Mono, xft:Symbola

! URxvt
URxvt.letterSpace:               1
URxvt.scrollBar:                 false
URxvt.perl-ext-common:           default,clipboard
URxvt.keysym.Shift-Control-V :   eval:paste_clipboard
URxvt.keysym.Shift-Control-C:    eval:selection_to_clipboard
